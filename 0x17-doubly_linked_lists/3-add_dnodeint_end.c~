#include "lists.h"

dlistint_int *add_dnodeint_end(dlistint_t **head, const int n)
{
	dlistint_t new_node;

	new_node = malloc(sizeof(dlistint_t));
	if (new_node == NULL)
		return (NULL);

	new_node->n = n;
	for (; *head != NULL; )
		*head = h->next;
	h->next = new_node;
	new_node->next = NULL;
	new_node->prev = *head;

	return (new_node);
}
